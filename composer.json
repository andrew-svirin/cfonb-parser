{
    "name": "andrew-svirin/cfonb-parser",
    "description": "A PHP Parser for CFONB 120 signatures, 240 transfers. Helper for EBICS.",
    "license": "MIT",
    "type": "library",
    "keywords": ["cfonb", "parser", "etebac", "swift", "ebics"],
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr"
        },
        {
            "name": "Andrew Svirin"
        }
    ],
    "require": {
        "php": "^7.2 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpstan/phpstan": "^0.12.57",
        "infection/infection": "^0.17.7|^0.20.2"
    },
    "autoload": {
        "psr-4": {
            "Silarhi\\Cfonb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\Cfonb\\Tests\\": "tests/"
        }
    }
}
